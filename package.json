{
  "name": "backend_task",
  "version": "1.0.0",
  "description": "random joke task",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seq:bootstrap": "npx sequelize-cli init",
    "seq:model": "npx sequelize-cli model:generate",
    "seq:migrate": "npx sequelize-cli db:migrate",
    "seq:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seq:migration:skeleton": "npx sequelize-cli migration:generate --name migration-skeleton",
    "seq:seed": "npx sequelize-cli db:seed:all",
    "seq:seed:undo": "npx sequelize-cli db:seed:undo",
    "seq:seed:skeleton": "npx sequelize-cli seed:generate --name demo-user"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FIvicproFico/Backend_Task.git"
  },
  "author": "Filip Ivic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FIvicproFico/Backend_Task/issues"
  },
  "homepage": "https://github.com/FIvicproFico/Backend_Task#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "sequelize": "^6.15.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.4.1"
  }
}
